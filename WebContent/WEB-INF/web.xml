<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">

<display-name>Flight Reservations</display-name>
	<welcome-file-list>
	  <welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	  
	<servlet>
    	<servlet-name>LogonServlet</servlet-name>
    	<servlet-class>edu.rutgers.cs539.LogonForm</servlet-class>
	</servlet>

	<servlet-mapping>
    	<servlet-name>LogonServlet</servlet-name>
    	<url-pattern>/Logon</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>RegisterServlet</servlet-name>
    	<servlet-class>edu.rutgers.cs539.RegisterForm</servlet-class>
	</servlet>

	<servlet-mapping>
    	<servlet-name>RegisterServlet</servlet-name>
    	<url-pattern>/Register</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>MyAccountServlet</servlet-name>
    	<servlet-class>edu.rutgers.cs539.MyAccountForm</servlet-class>
	</servlet>

	<servlet-mapping>
    	<servlet-name>MyAccountServlet</servlet-name>
    	<url-pattern>/MyAccountDetails</url-pattern>
	</servlet-mapping>
	
	
	
	
	
	<servlet>
    	<servlet-name>MyAccountJSP</servlet-name>
    	<jsp-file>/myAccount.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
    	<servlet-name>MyAccountJSP</servlet-name>
    	<url-pattern>/myAccount</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>BookYourTravelJSP</servlet-name>
    	<jsp-file>/bookYourTravel.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
    	<servlet-name>BookYourTravelJSP</servlet-name>
    	<url-pattern>/bookYourTravel</url-pattern>
	</servlet-mapping>
	
</web-app> 